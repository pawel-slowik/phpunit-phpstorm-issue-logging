{
    "name": "pawel-slowik/phpunit-phpstorm-issue-logging",
    "description": "Show PHPUnit's test warnings / notices / deprecations in PhpStorm UI. Workaround for PHPUnit issue #4711",
    "type": "library",
    "keywords": ["testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Słowik",
            "email": "pawel.slowik@zoho.com"
        }
    ],
    "require": {
        "php": "8.1 - 8.5",
        "phpunit/phpunit": "^10.0|^11.0|^12.0|^13.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PawelSlowik\\PhpunitPhpstormIssueLogging\\": "src/"
        }
    }
}
